{
    "name": "silverstripe/recipe-authoring-tools",
    "type": "silverstripe-recipe",
    "description": "Extra tools for CMS authoring in SilverStripe",
    "homepage": "https://www.silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "^4",
        "silverstripe/documentconverter": "2.1.x-dev",
        "silverstripe/iframe": "2.1.x-dev",
        "silverstripe/spellcheck": "2.2.x-dev",
        "silverstripe/tagfield": "2.6.x-dev",
        "silverstripe/taxonomy": "2.2.x-dev"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7"
    },
    "extra": {
        "project-files": []
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
